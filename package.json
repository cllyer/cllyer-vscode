{
	"name": "cllyer-snippets",
	"displayName": "cllyer-snippets",
	"description": "Snippets for Cllyer",
	"version": "0.0.1",
	"publisher":"cllyer",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
		 	}, {
				"language": "vue",
				"path": "./snippets/vue.json"
			}, {
				"language": "vue-html",
				"path": "./snippets/vue-html.json"
			}
		],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"extensionPack": [
		"ms-ceintl.vscode-language-pack-zh-hans",
		"formulahendry.auto-close-tag",
		"formulahendry.auto-rename-tag",
		"coenraads.bracket-pair-colorizer",
		"dbaeumer.vscode-eslint",
		"ecmel.vscode-html-css",
		"abusaidm.html-snippets",
		"ritwickdey.liveserver",
		"christian-kohler.path-intellisense",
		"octref.vetur",
		"eamodio.gitlens",
		"elemefe.vscode-element-helper",
		"mikey.vscode-fileheader",
		"vscode-icons-team.vscode-icons"
	],
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
